{
  "name": "my-event-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run sync-sitemap && npm install tailwindcss-animate && vite build && npm run build:beta",
    "build:beta": "cd ../beta && npm ci && npm run build && mkdir -p ../frontend/dist/beta && cp -r dist/* ../frontend/dist/beta/",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sync-sitemap": "node sync-sitemap.cjs",
    "verify-sitemap": "node verify-sitemap-sync.cjs",
    "build:render": "npm install tailwindcss-animate && vite build --mode production && npm run build:beta",
    "start": "npx serve -s dist",
    "generate-sitemap": "node generate-sitemap.js",
    "validate-sitemap": "node validate-sitemap.js"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.263.1",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1",
    "serve": "^14.2.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^4.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3"
  }
}
